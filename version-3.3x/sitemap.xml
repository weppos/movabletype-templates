<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9
         http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd"
         xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">

<!-- start homepage -->
<url>
  <loc><$MTBlogURL encode_xml="1"$></loc>
  <priority>1.0</priority>
  <changefreq>daily</changefreq>
  <MTEntries lastn="1">
  <lastmod><$MTEntryModifiedDate utc="1" format="%Y-%m-%dT%H:%M:%SZ"$></lastmod>
  </MTEntries>
</url>
<!-- end homepage -->

<!-- top100 entries start-->
<MTEntries lastn="100">
<url>
<loc><$MTEntryPermalink encode_xml="1"$></loc>
<lastmod><MTIfNonZero tag="MTEntryCommentCount"><MTComments lastn="1"><$MTCommentDate utc="1" format="%Y-%m-%dT%H:%M:%SZ"$></MTComments><MTElse><$MTEntryModifiedDate utc="1" format="%Y-%m-%dT%H:%M:%SZ"$></MTElse></MTIfNonZero>
</lastmod>
</url>
</MTEntries>
<!-- top100 entries end -->

<!-- entries start-->
<MTEntries lastn="9999" offset="100">
<url>
<loc><$MTEntryPermalink encode_xml="1"$></loc>
<lastmod><MTIfNonZero tag="MTEntryCommentCount"><MTComments lastn="1"><$MTCommentDate utc="1" format="%Y-%m-%dT%H:%M:%SZ"$></MTComments><MTElse><$MTEntryModifiedDate utc="1" format="%Y-%m-%dT%H:%M:%SZ"$></MTElse></MTIfNonZero>
</lastmod>
</url>
</MTEntries>
<!-- entries end -->

<!-- Monthly archive start -->
<MTIfArchiveTypeEnabled archive_type="Monthly">
<MTArchiveList archive_type="Monthly">
<url>
<loc><$MTArchiveLink$></loc>
</url>
</MTArchiveList>
</MTIfArchiveTypeEnabled>
<!-- Monthly archive end -->

<!-- Category archive start -->
<MTIfArchiveTypeEnabled archive_type="Category"><MTTopLevelCategories>
<MTIfNonZero tag="MTCategoryCount">
<url>
<loc><$MTCategoryArchiveLink$></loc>
</url>
</MTIfNonZero>
<MTSubCatsRecurse></MTTopLevelCategories></MTIfArchiveTypeEnabled>
<!-- Category archive end -->

</urlset>
